<h1>Replays</h1>

<p>(<strong class="highlight">{{replays.length}}</strong> of them)</p>

<ul class="replay-list">
  {{#each replays}}
  <li class="replay">
    <span class="game-id">
      <a href="/replays/{{value._id}}">{{value.gameId}}</a>
    </span>
    <span class="teams">{{#each value.scores}} <span>{{teamName}}</span> {{/each}}</span>
  </li>
  {{/each}}
</ul>